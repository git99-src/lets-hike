<nav class="navbar navbar-expand-md">
  <a class="navbar-brand" href="#">Let's Hike</a>
  <button class="navbar-toggler navbar-dark" type="button" data-toggle="collapse" data-target="#main-navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="main-navigation">
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" href="/search">Home</a>
      </li>
    </ul>
  </div>
</nav>
<div class="hero-image-top">
</div>
<div class="container">
  <div class="row">
    <div class="col-md-6 mx-auto col-sm-12">
      <h2>Seven day weather for {{name}}</h2>
      <h4>Current Weather:</h4>
      <ul>
        <li>Current Temperature: {{ current.temp }}</li>
        <li>Humidity: {{ current.humidity }}</li>
        <li>UVI: {{ current.uvi }}</li>
        <li>Conditions: {{current.condition}}</li>
      </ul>
      <ul class="list-group weather-list">
        {{#each weather}}
        <li class="media">
          <div class="media-body">
            <h4>Day: {{date}}</h4>
            <p>High temp: {{high}}</p>
            <p>Low temp: {{low}}</p>
            <p>Conditions: {{conditions}}</p>
          </div>
        </li>
        {{/each}}
        <br>
      </ul>
    </div>
  </div>
</div>